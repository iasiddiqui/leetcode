/*
Given an integer x, return true if x is a palindrome, and false otherwise.
*/


/**
 * @param {number} x
 * @return {boolean}
 */
var isPalindrome = function(x) {
    if (x < 0) return false;
    let str = x.toString();
    let j = str.length - 1;
    for (let i = 0; i < str.length / 2; i++) {
        if (str[i] !== str[j]){
            return false;
        }
        j--;
    }
    return true;
};
